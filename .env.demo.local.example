# Stable local demo profile (no external RPC/faucet dependency)
ALLOW_FREE_JOIN=true
REQUIRE_API_KEY=true
# Set at runtime (do NOT commit secrets): export WORLD_API_KEY='...'
# Intentionally not set here to avoid overriding shell env.
API_KEY_HEADER_NAME=X-API-Key
RATE_LIMIT_ENABLED=true
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_SEC=60
DEBUG_ENDPOINTS_ENABLED=false
DEMO_BASE_URL=http://127.0.0.1:8011
